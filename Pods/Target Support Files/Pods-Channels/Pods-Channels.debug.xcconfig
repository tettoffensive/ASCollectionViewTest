CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Bolts.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FLKAutoLayout.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/KVOController.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Mantle.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Mixpanel.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ObjectiveSugar.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/PBJVision.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/libextobjc.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/pop.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Bolts" -framework "FLKAutoLayout" -framework "KVOController" -framework "Mantle" -framework "Mixpanel" -framework "ObjectiveSugar" -framework "PBJVision" -framework "libextobjc" -framework "pop"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Channels
PODS_ROOT = ${SRCROOT}/../Pods